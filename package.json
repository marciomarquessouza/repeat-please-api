{
  "name": "repeat-please-api",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "debug": "env-cmd ./src/config/dev.env node --inspect bin/www",
    "dev": "env-cmd ./src/config/dev.env nodemon bin/www --exec 'yarn lint && node'",
    "uat": "env-cmd ./src/config/test.env nodemon bin/www --exec 'yarn lint && node'",
    "lint": "./node_modules/.bin/eslint ./src --ext .js",
    "start": "env-cmd ./src/config/dev.env node ./bin/www",
    "test": "nyc --reporter=html --reporter=text mocha --recursive",
    "coverage": "nyc report --reporter=text-lcov  | coveralls",
    "opencoverage": "google-chrome ./coverage/index.html"
  },
  "husky": {
    "hooks": {
      "pre-commit": "./node_modules/.bin/eslint ./src --ext .js",
      "pre-push": "./node_modules/.bin/eslint ./src --ext .js",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "dependencies": {
    "babel-eslint": "^10.0.1",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.18.3",
    "cookie-parser": "~1.4.3",
    "debug": "~2.6.9",
    "dotenv": "^6.2.0",
    "eslint": "5.15.0",
    "express": "~4.16.0",
    "global": "^4.3.2",
    "http-errors": "~1.6.2",
    "jsonwebtoken": "^8.5.0",
    "moment": "^2.24.0",
    "mongoose": "^5.4.19",
    "morgan": "~1.9.0",
    "request": "^2.88.0",
    "validator": "^10.11.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^7.5.2",
    "@commitlint/config-conventional": "^7.5.0",
    "chai": "^4.2.0",
    "coveralls": "^3.0.3",
    "env-cmd": "^8.0.2",
    "husky": "^1.3.1",
    "mocha": "^6.1.3",
    "nodemon": "^1.18.10",
    "nyc": "^14.0.0",
    "rimraf": "^2.6.3",
    "sinon": "^7.3.1",
    "supertest": "^4.0.2",
    "winston": "^3.2.1",
    "winston-daily-rotate-file": "^3.8.0"
  }
}
