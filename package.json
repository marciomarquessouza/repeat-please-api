{
  "name": "repeat-please-api",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "lint": "eslint ./src --ext .js",
    "dev": "nodemon bin/www --exec 'yarn lint && node'",
    "start": "./bin/www"
  },
  "husky": {
    "hooks": {
      "pre-commit": "eslint ./src --ext .js",
      "pre-push": "eslint ./src --ext .js",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "dependencies": {
    "cookie-parser": "~1.4.3",
    "debug": "~2.6.9",
    "dotenv": "^6.2.0",
    "express": "~4.16.0",
    "global": "^4.3.2",
    "http-errors": "~1.6.2",
    "jade": "~1.11.0",
    "morgan": "~1.9.0",
    "node-readme": "^0.1.9",
    "readme-generator": "^1.0.1",
    "request": "^2.88.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^7.5.2",
    "@commitlint/config-conventional": "^7.5.0",
    "husky": "^1.3.1",
    "jest": "^24.3.1",
    "nodemon": "^1.18.10"
  }
}
